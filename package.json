{
  "name": "durak-vue-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite && cd ./durak-server && npm run dev:respawn",
    "dev:concurrent": "conc  \"vite\" \"cd ./durak-server && npm run dev:respawn\" ",
    "build-ts": "run-p type-check build-only",
    "preview": "vite preview",
    "build": "vite build",
    "build:to-server": "rm -r ../durak-server/static/ && vite build && cp -rf ./dist/ ../durak-server/static/",
    "build:to-server2": "vite build && cp -rf ./dist/ ../durak-server/static/",
    "build:to-server3": "vite build && rm -r ../durak-server/static/ && cp -rf ./dist/ ../durak-server/static/",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "daisyui": "^3.5.1",
    "pinia": "^2.1.6",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "valibot": "^0.15.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@durak-game/durak-dts": "^0.4.10",
    "@rushstack/eslint-patch": "^1.3.3",
    "@savvywombat/tailwindcss-grid-areas": "^3.1.0",
    "@types/eslint": "^8.44.7",
    "@types/node": "20.5.1",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.41.00",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.3",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vue-tsc": "^1.8.8"
  }
}